{
	"builds": [{
			"src": "mysite/wsgi.py",
			"use": "@vercel/python",
			"config": {"maxLambdaSize": "15mb", "runtime": "python3.11" }
	}],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "mysite/wsgi.py"
		},
		{ "handle": "filesystem" },
    	{ "src": "/(.*)", "status": 404, "dest": "/other-404.html" }
	],

	"rewrites": [{ "source": "/api/(.*)", "destination": "/api/404.js" }],
}
