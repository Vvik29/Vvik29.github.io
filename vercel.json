{
  "version": 2,
  "name": "mysite",
  "alias": ["www.mysite.com", "mysite.com"],
  "env": {
    "API_KEY": "xxxxxx"
  },
  "build": {
    "command": "npm run build",
    "output": "dist"
  },
  "routes": [
    {
      "src": "/*",
      "dest": "https://mysite.com"
    },
      {
    "src": "/mysite/",
    "dest": "/mysite/*"
    } ,
      {
    "src": "/mysite/*",
    "dest": "urls.py"
    },

    {
      "src": "/static/(.*)",
      "dest": "/static/:splat"
    }
  ],
  "files": [
    "public/**/*",
    "src/**/*",
    "!src/tests/**/*"
  ]
}
